@page
@model ErrorModel
@{
    ViewData["Title"] = "Error";
}

<h3 class="text-danger display-3">Error.</h3>
<h6 class="text-danger display-6">An error occurred while processing your request.</h6>

@if (Model.ShowRequestId)
{
    <p>
        <strong>Request ID:</strong> <code>@Model.RequestId</code>
    </p>
}

<p class="fs-6">Please, send my attention to it by clicking <a asp-page="/Contact">Here</a> </p> 